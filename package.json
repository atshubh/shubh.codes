{
  "name": "shubh.codes",
  "private": false,
  "description": "Static site available at https://shubh.codes",
  "version": "0.0.0-development",
  "license": "MIT",
  "author": "Shubhanshu Aggarwal <shubhanshuworks@gmail.com>",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "commit": "cz",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "postinstall": "husky install",
    "publish": "gatsby build --prefix-paths && gh-pages -d public -b github-pages",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "gatsby": "^2.32.3",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-manifest": "^2.12.0",
    "gatsby-plugin-offline": "^3.10.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-sharp": "^2.14.1",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-transformer-sharp": "^2.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-webpack-plugin": "^2.5.1",
    "gatsby-cli": "^2.19.1",
    "gatsby-plugin-eslint": "^3.0.0",
    "gh-pages": "^3.1.0",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "semantic-release": "^17.3.9"
  },
  "keywords": [
    "gatsby",
    "shubh.codes",
    "web developer",
    "fullstack developer",
    "portfolio"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/atshubh/shubh.codes"
  },
  "bugs": {
    "url": "https://github.com/atshubh/shubh.codes/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,json,md}": "prettier --write"
  }
}
